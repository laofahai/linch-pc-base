{
  "productName": "linch-pc-base",
  "version": "0.0.1",
  "identifier": "tech.linch.base",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devUrl": "http://localhost:1450",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Linch PC Base",
        "width": 1200,
        "height": 800,
        "decorations": false,
        "transparent": false,
        "shadow": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
      "shell": {
          "open": true
      }
  }
}
